<!DOCTYPE html>
<html>
    <head>
        <title>Travnal</title>
        <meta charset="utf-8"> 
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <link rel="stylesheet" href="/stylesheets/main.css">
        <link rel="icon" href="/images/paper-plane.png">
        <script src="script.js"></script>
    </head>
    <body>
        <nav class="navbar navbar-inverse">
            <div class="container-fluid">
                <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>                        
                </button>
                    <a class="navbar-brand brand" href="#">Travnal</a>
                    <img class="logo-image" src="/images/paper-plane.png">
                </div>
                <div class="collapse navbar-collapse" id="myNavbar">
                <ul class="nav navbar-nav">
                    
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="active"><a href="/feed">Feed</a></li>
                    <li><a href="<%= timelinename %>">Trips</a></li>
                    <li ><a href="<%= profilename %>">Profile</a></li>
                    <li> <a href="/logout"> Logout </a></li>
                </ul>
                </div>
            </div>
        </nav>
        <div class="container-fluid">
        <div class="col-md-2"></div>  <!--search button-->
        <div class="col-md-8" id="profile-feed-sec">
                <section class="section">
                    <div class="grid">
                        <% for(var i=0; i<posts.length; i++) {%>
                            <div class="item <%= posts[i].class%>">
                                    <a href="/timeline/<%= name %>/<%= posts[i].post %>">
                                <div class="item__details">
                                    <%= posts[i].post %>
                                </div>
                                </a>
                            </div>
                            </a>
                        <%} %>
                    </div>
                </section>
        </div>
        <div class="col-md-2"></div>  <!--trending updates-->
        </div>
        
              <div class="footer">
        Developed with ‚ù§ by trotters at DJMB
    </div>
    
    </body>
</html>
